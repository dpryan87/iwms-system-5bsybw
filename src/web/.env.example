# API Configuration
# Backend API base URL - Required for all API communications
VITE_API_URL=http://localhost:3000
# API request timeout in milliseconds
VITE_API_TIMEOUT=30000
# API version for endpoint routing
VITE_API_VERSION=v1

# Authentication Configuration
# Auth0 domain - Required for SSO integration
VITE_AUTH_DOMAIN=your-auth0-domain.auth0.com
# Auth0 client ID - Required for application identification
VITE_AUTH_CLIENT_ID=your-auth0-client-id
# Auth0 API audience - Required for API authorization
VITE_AUTH_AUDIENCE=your-auth0-api-identifier

# WebSocket Configuration
# WebSocket server URL - Required for real-time updates
VITE_WEBSOCKET_URL=ws://localhost:3001

# Application Configuration
# Deployment environment (development/staging/production)
VITE_ENVIRONMENT=development
# Toggle for analytics integration
VITE_ENABLE_ANALYTICS=false